{
  "name": "brewbook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "algolia:config": "node scripts/algolia-config.js",
    "algolia:synonyms": "node -e \"require('./scripts/algolia-config.js').applySynonyms()\"",
    "algolia:settings": "node -e \"require('./scripts/algolia-config.js').applyIndexSettings()\"",
    "algolia:test": "node -e \"require('./scripts/algolia-config.js').testConfiguration()\"",
    "seed:storyblok": "python3 storyblok_seed.py",
    "seed:algolia": "node seed-algolia.js",
    "seed:algolia:replace": "node seed-algolia.js --replace-all",
    "seed:all": "npm run seed:storyblok && npm run seed:algolia",
    "seed:all:replace": "npm run seed:storyblok && npm run seed:algolia:replace"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@storyblok/react": "^5.4.11",
    "algoliasearch": "^5.37.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-instantsearch": "^7.16.3",
    "storyblok-rich-text-react-renderer": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5"
  }
}
